package br.com.sisloc.model;

/**
 *
 * @author Tiago
 */
public class Item {

    private int idLocacao,idProduto,quantidade,qt_dias;

    public Item(int idLocacao, int idProduto, int quantidade, int qt_dias) {
        this.idLocacao = idLocacao;
        this.idProduto = idProduto;
        this.quantidade = quantidade;
        this.qt_dias = qt_dias;
    }

    /**
     * @return the idLocacao
     */
    public int getIdLocacao() {
        return idLocacao;
    }

    /**
     * @param idLocacao the idLocacao to set
     */
    public void setIdLocacao(int idLocacao) {
        this.idLocacao = idLocacao;
    }

    /**
     * @return the idProduto
     */
    public int getIdProduto() {
        return idProduto;
    }

    /**
     * @param idProduto the idProduto to set
     */
    public void setIdProduto(int idProduto) {
        this.idProduto = idProduto;
    }

    /**
     * @return the quantidade
     */
    public int getQuantidade() {
        return quantidade;
    }

    /**
     * @param quantidade the quantidade to set
     */
    public void setQuantidade(int quantidade) {
        this.quantidade = quantidade;
    }

    /**
     * @return the qt_dias
     */
    public int getQt_dias() {
        return qt_dias;
    }

    /**
     * @param qt_dias the qt_dias to set
     */
    public void setQt_dias(int qt_dias) {
        this.qt_dias = qt_dias;
    }

   
    
}
